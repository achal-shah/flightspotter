{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/FlightSpotter.Web/FlightSpotter.Web.csproj"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "run",
      "command": "dotnet",
      "type": "process",
      "args": [
        "run",
        "--project",
        "${workspaceFolder}/FlightSpotter.Web",
        "--urls",
        "http://localhost:5000"
      ],
      "isBackground": true,
      "problemMatcher": []
    }
  ]
}
